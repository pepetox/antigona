%h1
  = link_to @integrante.name, integrante_path(@integrante)
%p
  - unless @integrante.img_link == ""
    = image_tag @integrante.img_link, :size => "200x200", class: "img-rounded", id: "foto-integrante"
  %p= @integrante.description.html_safe
  %p= link_to "link al curriculum", @integrante.cv_link
  = l @integrante.created_at
